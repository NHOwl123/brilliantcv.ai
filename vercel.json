{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist/public", 
  "installCommand": "npm install",
  "functions": {
    "dist/index.js": {
      "runtime": "@vercel/node@3.0.7"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/dist/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
